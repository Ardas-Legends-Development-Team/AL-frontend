{
  "name": "ardas-legends-website",
  "private": true,
  "version": "Alpha 0.2.3",
  "scripts": {
    "dev": "vite",
    "lint": "eslint .",
    "build": "npx rimraf tsconfig.tsbuildinfo && vue-tsc --noEmit && vite build",
    "preview": "npx rimraf tsconfig.tsbuildinfo && vite preview",
    "production": "npx rimraf tsconfig.tsbuildinfo && vue-tsc --noEmit && vite build && vite preview",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "typecheck": "npx rimraf tsconfig.tsbuildinfo && vue-tsc --noEmit",
    "coverage": "vitest run --coverage",
    "docker:build": "docker build -t vernonroche/al-website-docker .",
    "docker:run": "docker run -p 8081:3333 vernonroche/al-website-docker"
  },
  "dependencies": {
    "@vueuse/core": "^10.3.0",
    "axios": "^1.4.0",
    "cssnano": "^6.0.1",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-cookie-next": "^1.3.0",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@iconify/vue": "^4.1.1",
    "@rushstack/eslint-patch": "^1.3.2",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.14",
    "cypress": "^12.17.2",
    "cypress-vite": "^1.4.2",
    "daisyui": "^3.5.0",
    "eslint": "^8.46.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-vue": "^9.16.1",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.27",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vite-bundle-visualizer": "^0.10.0",
    "vite-plugin-webfont-dl": "^3.7.6",
    "vitest": "^0.33.0",
    "vue-tsc": "^2.1.6"
  }
}
